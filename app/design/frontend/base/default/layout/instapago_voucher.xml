<layout>
    
    <instapago_voucher_voucher_index translate="label" module="instapago_voucher">
        <label>Voucher list</label>
        <update handle="page_two_columns_left" />
        <reference name="content">
            <block type="instapago_voucher/voucher_list" name="voucher_list" template="instapago_voucher/voucher/list.phtml" />
        </reference>
    </instapago_voucher_voucher_index>
    <instapago_voucher_voucher_view translate="label" module="instapago_voucher">
        <label>Voucher view page</label>
        <update handle="page_two_columns_left" />
        <reference name="content">
            <block type="instapago_voucher/voucher_view" name="voucher_view" template="instapago_voucher/voucher/view.phtml" />
        </reference>
    </instapago_voucher_voucher_view>
</layout>
